UNWIND [{start: {_id:15}, end: {_id:17}, properties:{}}, {start: {_id:19}, end: {_id:21}, properties:{}}, {start: {_id:23}, end: {_id:25}, properties:{}}, {start: {_id:27}, end: {_id:29}, properties:{}}] AS row
